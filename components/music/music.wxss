.container{
  display: flex;
  flex-direction: row;
  height: 100rpx;
  padding: 0 20rpx;
  border-bottom: 1px solid #eee;
}
.index{
  font-size: 36rpx;
  margin: 20rpx 20rpx 0 0;
}
.funny-text{
  font-size: 30rpx;
}
.play{
  width: 44rpx;
  height: 44rpx;
  margin-top: 30rpx;
}
.music-details{
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.music-detail{
  display: flex;
  flex-direction: column;
  margin-top: 10rpx;
}
.songname{
  font-size: 34rpx;
}
.singername{
  font-size: 24rpx;
  color: #888;
}